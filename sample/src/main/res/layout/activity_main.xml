<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.benoitletondor.mvp.maps.sample.scene.main.impl.MainActivity">

    <Button
        android:id="@+id/start_fragment_activity_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="@string/start_fragment_activity_cta"/>

    <FrameLayout
        android:id="@+id/activity_map_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>
